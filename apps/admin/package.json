{
  "name": "@bavaan/admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "populate": "ts-node populate.ts",
    "compile-admin-ui": "ts-node src/compile-admin-ui.ts",
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "start": "concurrently yarn:run:*",
    "build": "rimraf dist && rimraf admin-ui && tsc && tsc -p tsconfig.migrations.json && yarn compile-admin-ui",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "e2e:reviews": "jest --config src/plugins/reviews/e2e/config/jest-config.js"
  },
  "dependencies": {
    "@bavaan/vendure-reviews-plugin": "^0.0.1",
    "@bavaan/vendure-social-auth-plugin": "^0.0.1",
    "@bavaan/vendure-vendor-plugin": "^0.0.1",
    "@bavaan/vendure-tier-price-plugin": "^0.0.1",
    "@bavaan/vendure-product-recommendations-plugin": "^0.0.1",
    "@bavaan/vendure-braintree-payment-plugin": "^0.0.1",
    "@bavaan/vendure-domestic-shipping-plugin": "^0.0.1",
    "@vendure/admin-ui-plugin": "^0.13.0",
    "@vendure/asset-server-plugin": "^0.13.0",
    "@vendure/core": "^0.13.0",
    "@vendure/email-plugin": "^0.13.0",
    "@vendure/ui-devkit": "^0.13.0",
    "mysql": "^2.18.1",
    "pg": "^8.2.1",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.8.3",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-compatibility": "^1.8.3",
    "@graphql-codegen/typescript-operations": "^1.8.3",
    "@types/braintree": "^2.20.0",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@vendure/create": "0.12.5",
    "@vendure/testing": "0.12.5",
    "concurrently": "5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "8.5.2",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "tslib": "1.11.2"
  }
}
